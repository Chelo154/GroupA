@{
    ViewBag.Title = "Acerca de";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<p>Use this area to provide additional information.</p>
<div class="container">
    <div class="row">
        <div class="col-sm-12">
            <h1>Mision</h1>
            <h3><p>@ViewBag.Mision</p></h3>
        </div>
        <div class="col-sm-12">
            <h1>Vision</h1>
            <h3><p>@ViewBag.Vision</p></h3>
        </div>
        <div class="col-sm-12">
            <h1>Objetivos</h1>
            
                @foreach (String objetivo in ViewBag.Objetivos)
                {
                    <button class="btn-default">
                        @objetivo
                    </button>    
                }
           
        </div>


    </div>
</div>
